      "isDistro": true,
      "rsync": {
        "host": "rsync.osuosl.org",
        "src": "debian",
        "dest": "/storage/debian",
        "options": "--exclude Packages* --exclude Sources* --exclude Release* --exclude=InRelease --include=i18n/by-hash/** --exclude=i18n/* --exclude ls-lR*",
