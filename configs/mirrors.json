{
  "$schema": "./mirrors.schema.json",
  "mirrors": {
    "almalinux": {
      "name": "AlmaLinux",
      "isDistro": true,
      "rsync": {
        "host": "rsync.repo.almalinux.org",
        "src": "alma",
        "dest": "/storage/almalinux",
        "options": "-avzrH --numeric-ids --delete --delete-delay --delay-updates --ignore-errors --exclude \".~tmp~\"",
        "syncs_per_day": 8
      },
      "official": false,
      "homepage": "https://almalinux.org",
      "color": "#7851a9",
      "publicRsync": true
    },
    "alpine": {
      "name": "Alpine Linux",
      "isDistro": true,
      "rsync": {
        "host": "rsync.alpinelinux.org",
        "src": "alpine",
        "dest": "/storage/alpine",
        "options": "-avzrH --numeric-ids --delete --delete-delay --delay-updates --ignore-errors --exclude \".~tmp~\"",
        "sync_file": "last-updated",
        "syncs_per_day": 4
      },
      "official": false,
      "homepage": "https://www.alpinelinux.org/",
      "color": "#cd5700",
      "publicRsync": true
    },
    "archlinux": {
      "name": "Arch Linux",
      "isDistro": true,
      "rsync": {
        "host": "mirrors.rit.edu",
        "src": "archlinux",
        "dest": "/storage/archlinux",
        "options": "-vrlptH --safe-links --delete-delay --delay-updates",
        "syncs_per_day": 12
      },
      "official": true,
      "homepage": "http://www.archlinux.org/",
      "color": "#f0d4f0",
      "publicRsync": true
    },
    "archlinux32": {
      "name": "Arch Linux 32",
